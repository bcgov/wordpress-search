{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wordpress-search/search-metadata-filter",
	"version": "1.0.0",
	"title": "Metadata Filter",
	"category": "search",
	"icon": "filter",
	"description": "A modular search metadata filter block that connects to the search page template",
	"attributes": {
		"selectedMetadata": {
			"type": "string",
			"default": ""
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewStyle": "file:./view.css",
	"viewScript":"file:./view.js",
	"render": "file:./render.php",
	"style": "file:./style-index.css",
	"dependencies": [
		"wp-api-fetch",
		"wp-url",
		"wp-components",
		"wp-element",
		"wp-i18n"
	]
}
