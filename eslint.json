{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "error",
        "no-unused-vars": "error", 
        "react/prop-types": "off" 
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}